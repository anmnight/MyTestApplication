<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anxiao.mytestapplication">

    <!-- 允许程序打开网络套接字 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 允许程序设置内置sd卡的写权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 允许程序获取网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 允许程序访问WiFi网络信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 允许程序读写手机状态和身份 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <!-- 允许程序访问CellID或WiFi热点来获取粗略的位置 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".HomeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- hotfix -->
        <meta-data
            android:name="com.taobao.android.hotfix.IDSECRET"
            android:value="118541-1" />
        <meta-data
            android:name="com.taobao.android.hotfix.APPSECRET"
            android:value="9cc3ef521f9c53ca6d205d52d32bbdc7" />
        <meta-data
            android:name="com.taobao.android.hotfix.RSASECRET"
            android:value="" />

        <!-- 高德地图 -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="658113e9bbb37fab156cebc70c9ea45e" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lesson_java.ListIterator" />
        <activity
            android:name=".lesson_gaodemap.BaseMapView"
            android:label="@string/title_activity_base_map_view"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".lesson_http.HttpTest"
            android:label="@string/title_activity_http_test"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".lesson_fresco.FrescoActivity"
            android:label="@string/title_activity_fresco"
            android:theme="@style/AppTheme" />
        <activity android:name=".lesson_fresco.ThumbActivity" />
        <activity android:name=".customer_drawable.XfermodeRoundImageActivity"></activity>
    </application>

</manifest>